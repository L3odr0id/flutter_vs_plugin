{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"baseUrl": "src",
		"outDir": "out",
		"paths": {
			"@/*": ["./*"]
		},
		"sourceMap": true,
		"newLine": "lf",
		"strict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"exactOptionalPropertyTypes": true,
	},
}
